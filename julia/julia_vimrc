set ruler
set number
set autoindent
set noerrorbells
set title
set shell=/bin/bash

" Highlight cursor line underneath the cursor horizontally.
set cursorline

" Highlight cursor line underneath the cursor vertically.
set cursorcolumn

runtime macros/matchit.vim 

call plug#begin()

Plug 'JuliaEditorSupport/julia-vim'

call plug#end()
